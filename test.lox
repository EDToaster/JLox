fun fib(n) {
  if (n <= 1) return n;
  return fib(n - 2) + fib(n - 1);
}

var start = time();
for (var i = 0; i < 10; i = i + 1) {
  fib(i);
}
var end = time();

print("" + (end - start) + " milliseconds elapsed");

